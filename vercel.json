{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "crons": [
      {
        "path": "/api/calculate-tps",
        "schedule": "0 0 * * *"
      }
    ]
  }